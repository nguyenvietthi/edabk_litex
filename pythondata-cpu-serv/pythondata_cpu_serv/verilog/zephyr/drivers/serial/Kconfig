if SERIAL

menuconfig UART_BITBANG
	bool "SERV bitbang serial driver"
	select SERIAL_HAS_DRIVER
	help
	  Enables the SERV bitbang serial driver.
	  Hardcoded to send at ~57600 baud rate with a 16MHz clock

endif
